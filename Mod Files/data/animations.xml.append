
<animSheet name="ghost" w="315" h="455" fw="35" fh="35">people/ghost_base.png</animSheet>


<anim name="ghost_clone">
	<sheet>ghost</sheet>
	<desc length="6" x="0" y="2"/>
	<time>0.75</time>
</anim>


<anim name="ghost_death_right">
	<sheet>ghost</sheet>
	<desc length="5" x="3" y="0"/>
	<time>0.75</time>
</anim>

<anim name="ghost_death_left">
	<sheet>ghost</sheet>
	<desc length="5" x="0" y="2"/>
	<time>0.75</time>
</anim>

<anim name="ghost_walk_down">
	<sheet>ghost</sheet>
	<desc length="4" x="0" y="12"/>
	<time>1.0</time>
</anim>

<anim name="ghost_walk_right">
	<sheet>ghost</sheet>
	<desc length="4" x="4" y="12"/>
	<time>1.0</time>
</anim>

