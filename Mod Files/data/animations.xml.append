	<sheet>ghost</sheet>
	<desc length="4" x="0" y="11"/>
	<time>1.0</time>
</anim>

<anim name="ghost_walk_left">
	<sheet>ghost</sheet>
	<desc length="4" x="4" y="11"/>
	<time>1.0</time>
</anim>

<anim name="ghost_shoot_down">
	<sheet>ghost</sheet>
	<desc length="3" x="0" y="10"/>
	<time>1.0</time>
</anim>

<anim name="ghost_shoot_right">
	<sheet>ghost</sheet>
	<desc length="3" x="3" y="10"/>
	<time>1.0</time>
</anim>

<anim name="ghost_shoot_up">
	<sheet>ghost</sheet>
	<desc length="3" x="6" y="10"/>
	<time>1.0</time>
</anim>

<anim name="ghost_shoot_left">
	<sheet>ghost</sheet>
	<desc length="3" x="0" y="1"/>
	<time>1.0</time>
</anim>

<anim name="ghost_fire_down">
	<sheet>ghost</sheet>
	<desc length="4" x="0" y="9"/>
	<time>1.0</time>
</anim>

<anim name="ghost_fire_right">
	<sheet>ghost</sheet>
	<desc length="4" x="4" y="9"/>
	<time>1.0</time>
</anim>

<anim name="ghost_fire_up">
	<sheet>ghost</sheet>
	<desc length="4" x="0" y="8"/>
	<time>1.0</time>
</anim>

<anim name="ghost_fire_left">
	<sheet>ghost</sheet>
	<desc length="4" x="4" y="8"/>
	<time>1.0</time>
</anim>

<anim name="ghost_repair">
	<sheet>ghost</sheet>
	<desc length="9" x="0" y="7"/>
	<time>1.0</time>
</anim>

<anim name="ghost_punch_up">
	<sheet>ghost</sheet>
	<desc length="6" x="3" y="1"/>
	<time>1.0</time>
</anim>

<anim name="ghost_punch_down">
	<sheet>ghost</sheet>
	<desc length="6" x="0" y="6"/>
	<time>1.0</time>
</anim>

<anim name="ghost_teleport">
	<sheet>ghost</sheet>
	<desc length="9" x="0" y="3"/>
	<time>0.5</time>
</anim>

<anim name="ghost_type_right">
	<sheet>ghost</sheet>
	<desc length="4" x="0" y="5"/>
	<time>0.75</time>
</anim>
<anim name="ghost_type_left">
	<sheet>ghost</sheet>
	<desc length="4" x="4" y="5"/>
	<time>0.75</time>
</anim>
<anim name="ghost_type_up">
	<sheet>ghost</sheet>
	<desc length="4" x="0" y="4"/>
	<time>0.75</time>
</anim>
<anim name="ghost_type_down">
	<sheet>ghost</sheet>
	<desc length="4" x="4" y="4"/>
