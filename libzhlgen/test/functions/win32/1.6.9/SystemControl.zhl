struct SystemControl
{{
    bool IsSystemTapped(int systemId)
    {
        for (SystemBox* pSystemBox : sysBoxes)
        {
            if (pSystemBox->pSystem->iSystemType == systemId)
            {
                return pSystemBox->tapped;
            }
        }
        return false;
    }
}};
